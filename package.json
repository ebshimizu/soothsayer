{
  "name": "soothsayer",
  "productName": "soothsayer",
  "version": "1.5.0",
  "description": "Soothsayer: A tool for generating OBS overlays for casting some video games.",
  "main": "src/index.js",
  "keywords": [],
  "author": "falindrith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebshimizu/soothsayer.git"
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.0.6",
    "express": "^4.16.3",
    "extract-zip": "^1.6.7",
    "fs-extra": "^7.0.0",
    "hots-parser-fallback": "^7.38.2",
    "jquery": "^3.3.1",
    "level-js": "^2.2.4",
    "linvodb3": "^3.25.1",
    "medea": "^1.0.3",
    "medeadown": "^1.1.9",
    "moment": "^2.23.0",
    "request": "^2.88.0",
    "semantic-ui": "^2.4.2",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "electron": "^4.0.1",
    "electron-builder": "^20.11.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "15",
    "eslint-plugin-import": "2",
    "eslint-plugin-jsx-a11y": "5",
    "eslint-plugin-react": "7"
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "electron ./",
    "upload": "electron-builder --x64 --publish always",
    "protocol": "node ./node_modules/heroprotocol/postinstall.js"
  },
  "build": {
    "appId": "com.soothsayer.app",
    "productName": "Soothsayer",
    "asar": false,
    "publish": [
      {
        "provider": "github",
        "private": false
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
